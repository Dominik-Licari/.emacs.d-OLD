package schoolwork; //this is a package, it holds classes under a common name.
public class helloworld //this is a class. It is a collection of methods and variables
{
    public static void main(String[] argsv) //this is the main method. This is the thing that the program runs at the start
    {
	prl("Hello world!"); //calling a method
    }
    public static void prl(String in) //making a method
    {
	System.out.println(in);
    }
}
